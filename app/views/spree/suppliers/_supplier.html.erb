<div id="supplier_<%= supplier.id %>" class="col-md-3 col-sm-6 product-list-item" data-hook="supplier_list_item" itemscope itemtype="http://schema.org/supplier">
	<div class="panel panel-default">
		<div class="panel-body text-center product-body">
			<%= image_tag supplier.profile_picture.url(:medium) %><br/>
			<%= link_to truncate(supplier.name, :length => 50), :class => 'info', :itemprop => "name", :title => supplier.name %>
		</div>
	</div>
</div>